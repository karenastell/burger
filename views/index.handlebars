<h1>Eat-Da-Burger</h1>

<div class="all-burgers">
<div class = "uneaten-burgers">
{{#each burger}}
<p>{{name}}</p> <button class = "eat" data-id="{{id}}">Eat-Da-Burger!</button>
{{/each}}
</div>
<div class = "devoured-burgers">
    {{#each burger}}
<p>{{name}}</p>
{{/each}}
</div>

</div>

<form>
    <input class="input-burger" type="text" name = "burgers" placeholder="What's your new burger??">
    <button class="submit-btn" type="submit">Submit</button>
</form>
